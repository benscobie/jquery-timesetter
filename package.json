{
  "name": "jquery-timesetter",
  "version": "1.0.0",
  "description": "A jQuery plugin which generates a UI component to take user inputs or to display time values with hour and minutes in a friendly manner. The UI provides intuitive behaviors for a better user experience such as real-time validation and keyboard arrow key support.",
  "keywords": [
    "front-end",
    "web",
    "jquery"
  ],
  "homepage": "https://github.com/benscobie/jquery-timesetter#readme",
  "author": "Sandun Angelo Perera",
  "contributors": [
    "Ben Scobie"
  ],
  "style": "dist/css/jquery.timesetter.css",
  "sass": "src/scss/jquery.timesetter.scss",
  "main": "dist/js/jquery.timesetter.js",
  "scripts": {
    "clean-css": "cleancss --level 1 --output dist/css/jquery.timesetter.min.css dist/css/jquery.timesetter.css",
    "uglify": "uglifyjs --output dist/js/jquery.timesetter.min.js dist/js/jquery.timesetter.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benscobie/jquery-timesetter.git"
  },
  "bugs": {
    "url": "https://github.com/benscobie/jquery-timesetter/issues"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "jquery": ">=1.7.0"
  },
  "files": [
    "dist",
    "src/js/*.js",
    "src/scss/*.scss",
    "example.html",
    "Gruntfile.js",
    "LICENSE"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "clean-css-cli": "^4.0.9",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-jshint": "^1.1.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^2.0.0",
    "grunt-sass": "^2.0.0",
    "load-grunt-tasks": "^3.5.2",
    "uglify-js": "^2.8.14"
  }
}
